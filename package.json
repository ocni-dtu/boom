{
  "name": "boom-speckle",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.yaml",
    "lint": "eslint src/**/*.{tsx,ts}",
    "lint:fix": "eslint --fix src/**/*.{tsx,ts}",
    "format": "prettier --check \"**/*.{ts,tsx}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx}\"",
    "tsc": "tsc --noEmit",
    "check": "npm run format && npm run lint && npm run tsc",
    "fix": "npm run lint:fix && npm run format:fix"
  },
  "dependencies": {
    "@fontsource/outfit": "^5.0.8",
    "@mantine/core": "^7.3.2",
    "@mantine/hooks": "^7.3.2",
    "@speckle/objectloader": "^2.17.16",
    "@tabler/icons-react": "^2.44.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@apollo/client": "^3.9.2",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-react-apollo": "^4.2.0",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "postcss-preset-mantine": "^1.12.2",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.2.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-tsconfig-paths": "^4.2.2"
  }
}
